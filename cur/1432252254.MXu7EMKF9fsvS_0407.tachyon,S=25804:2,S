var x;
function fnQuickSubmit(x)
{

  if (checkNoNull(document.frmquickdocs.Product,'Product',false))

  {   
  
  if (document.frmquickdocs.Product.value == 'Enter Product Grade') 
  {
     alert('Please enter a Product Name');
  }
  else
  {
    document.frmquickdocs.target='_new';
    if (x=='1')
    {
    document.frmquickdocs.action='https://www.gesilicones.com/NASApp/gesilprotectedtrans/PSQuickDataSheet';
    document.frmquickdocs.submit();
    }
    if (x=='2')
    {
    document.frmquickdocs.action='/NASApp/gesilecomm/SQuickMsds';
    document.frmquickdocs.submit();
    }
  }
  }
}

</SCRIPT>
