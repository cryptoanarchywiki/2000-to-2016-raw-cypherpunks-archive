function Is(){
	this.ver=navigator.appVersion
	this.dom=document.getElementById?1:0
	this.minor=parseFloat(navigator.appVersion)

	this.ie5=(this.ver.indexOf("MSIE 5")>-1 & this.dom)?1:0;
	this.ie4=(document.all & !this.dom)?1:0;
	this.ie=(this.ie5 || this.ie4);

	this.ns6=(this.dom & parseInt(this.ver) >= 5) ?1:0;
	this.ns4=(document.layers & !this.dom)?1:0;
	this.ns4b=(document.layers & !this.dom & this.minor < 4.04)?1:0
	this.ns=(this.ns4 || this.ns6);

	this.compat=(this.ie5 || this.ie4 || this.ns4 || this.ns6);
	return this;
}

is = new Is();

function createWin(popURL, winName)
{
    var mycookie, cookie1;
    var bgotuser = false;
    var index;
	var game_name, name_length;
	var wfeature;
	if(is.ns4) {
        var avwidth=innerWidth;
        var avheight=innerHeight;

    } else if(is.ie4 || is.ie5) {
        var avwidth=document.body.clientWidth;
        var avheight=document.body.clientHeight;

    }

	var ptop = (avheight-400)/2;
	var pleft = (avwidth-560)/2;
	name_length = popURL.length;
	game_name = popURL.substring (name_length-10, name_length-7);

	wfeature = "toolbar=0,location=0,directories=0,status=0," +
            "menubar=0,width=560,height=405,screenX=15,screenY=15,top="+ptop+",left="+pleft+",scrollbars=no,resizable=no";

    mycookie  = document.cookie;

    // do our own split, so that we work on IE 3.0

    // location.href="/members/casino_games.html";

    index     = mycookie.indexOf (";");

    // replace ; with &
    while (mycookie.length > 0)
    {
        if (index == -1)
            index = mycookie.length;
        cookie1 = mycookie.substring (0, index);
        mycookie = mycookie.substring (index+1);

        if (cookie1.charAt(0) == ' ')
            cookie1 = cookie1.substring (1);
        if (!bgotuser & (cookie1.substring (0, 8) == "username"
            || cookie1.substring (0, 8) == "userName"))
        {
            bgotuser = true;
            popURL = popURL + "&" + cookie1;
        }
        if (cookie1.substring (0, 9) == "sessionID")
            popURL = popURL + "&" + cookie1;
        index     = mycookie.indexOf (";");
    }

    if (window.top.name.length == 0)
        window.top.name = "casino_win";

    popURL = popURL + "&opener=" + window.top.name;




 if (game_name == "=pp")
	{
	 wfeature = "toolbar=0,location=0,directories=0,status=0," +
            "menubar=0,width=560,height=515,screenX=15,screenY=15,top="+ptop+",left="+pleft+",scrollbars=auto,resizable=no";
 }

 if (game_name == "vip")
	{
	 wfeature = "toolbar=0,location=0,directories=0,status=0," +
            "menubar=0,width=760,height=555,screenX=15,screenY=15,top="+ptop+",left="+pleft+",scrollbars=auto,resizable=no";
 }

  msgWindow=window.open(popURL, winName, wfeature);

}



